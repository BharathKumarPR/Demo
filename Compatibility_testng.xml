<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="2" name="Suite" parallel="tests">


 <test name="Test-Firefox-Runner">
   <parameter name="BROWSER" value="firefox"></parameter>
    <classes>
      <class name="Organization.ValidateCSARadioBtn"/>
      <class name="Organization.ValidateRatingDropdown"/>
      <class name="Organization.ValidateNOCkeckBox"/>
      <class name="Organization.ValidateCBARadioBtn"/>
      <class name="Organization.createOrgWithNotifyOwner"/>
      <class name="Organization.createOrgWithRating"/>
    </classes>
  </test> <!-- Test -->
  
  <test name="Test-Chrome-Runner">
   <parameter name="BROWSER" value="chrome"></parameter>
    <classes>
      <class name="Organization.ValidateCSARadioBtn"/>
      <class name="Organization.ValidateRatingDropdown"/>
      <class name="Organization.ValidateNOCkeckBox"/>
      <class name="Organization.ValidateCBARadioBtn"/>
      <class name="Organization.createOrgWithNotifyOwner"/>
      <class name="Organization.createOrgWithRating"/>
    </classes>
  </test> <!-- Test -->
  
</suite> <!-- Suite -->
